<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="caotinging.domain">
 <class name="LinkMan" table="cst_linkman">
  <id name="lkm_id">
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" name="lkm_gender"/>
  <property generated="never" lazy="false" name="lkm_name"/>
  <property generated="never" lazy="false" name="lkm_phone"/>
  <property generated="never" lazy="false" name="lkm_mobile"/>
  <property generated="never" lazy="false" name="lkm_email"/>
  <property generated="never" lazy="false" name="lkm_qq"/>
  <property generated="never" lazy="false" name="lkm_position"/>
  <property generated="never" lazy="false" name="lkm_memo"/>
  <many-to-one class="caotinging.domain.Customer" column="lkm_cust_id" name="customer"/>
 </class>
</hibernate-mapping>
