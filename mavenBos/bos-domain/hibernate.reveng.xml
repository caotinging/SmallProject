<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="bc_decidedzone" catalog="maven"
		class="caotinging.domain.Decidedzone">
	</table>
	<table name="bc_region" catalog="maven"
		class="caotinging.domain.Region">
	</table>
	<table name="bc_staff" catalog="maven"
		class="caotinging.domain.Staff">
	</table>
	<table name="bc_subarea" catalog="maven"
		class="caotinging.domain.Subarea">
	</table>
	<table name="qp_noticebill" catalog="maven"
		class="caotinging.domain.Noticebill">
		<primary-key>
			<generator class="uuid"></generator>
		</primary-key>
	</table>
	<table name="qp_workbill" catalog="maven"
		class="captinging.domain.Workbill">
		<primary-key>
			<generator class="uuid"></generator>
		</primary-key>
	</table>
	<table name="qp_workordermanage" catalog="maven"
		class="caotinging.domain.Workordermanage">
		<primary-key>
			<generator class="uuid"></generator>
		</primary-key>
	</table>
	<table name="auth_function" catalog="maven"
		class="caotinging.domain.Function">
		<column name="pid" property="parentFunction"></column>
	</table>
	<table name="auth_role" catalog="maven" class="caotinging.domain.Role"></table>
</hibernate-reverse-engineering>