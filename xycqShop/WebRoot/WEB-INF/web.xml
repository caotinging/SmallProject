<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <servlet>
    <servlet-name>CheckImgServlet</servlet-name>
    <display-name>CheckImgServlet</display-name>
    <description>验证码生成</description>
    <servlet-class>caotinging.web.servlet.CheckImgServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <display-name>RegisterServlet</display-name>
    <description>用户注册</description>
    <servlet-class>caotinging.web.servlet.RegisterServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>CheckImgServlet</servlet-name>
    <url-pattern>/servlet/CheckImgServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/servlet/RegisterServlet</url-pattern>
  </servlet-mapping>

</web-app>